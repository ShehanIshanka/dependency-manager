# Routes
# Home page
GET     /                         controllers.DependencyController.index(request: Request)
GET     /reset                    controllers.DependencyController.reset
#GET     /search                    controllers.DependencyController.search(node:String)

# Widgets
POST    /upload                   controllers.DependencyController.upload(request: Request)
POST    /recoveryGraph            controllers.DependencyController.buildRecoveryGraph(request: Request)
# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file             controllers.Assets.versioned(path="/public", file: Asset)